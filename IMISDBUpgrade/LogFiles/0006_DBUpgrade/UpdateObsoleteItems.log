  FROM aspnet_Applications
 WHERE [LoweredApplicationName] = 'imis'
 
IF (@applicationId IS NOT NULL) AND EXISTS (SELECT 1 FROM INFORMATION_SCHEMA.TABLES WHERE [TABLE_NAME] = 'UserMain')
