SET NUMERIC_ROUNDABORT OFF

--------------------------------------------------------
-- Get the roster of a parent and its children using
-- the Name.CO_ID relationship. Returns the ID, plus
-- all the IDs whose CO_ID references ID, and so on
-- recursively for each of THEIR CO_ID references.
-- Also returns a 'depth' to show how far removed from
-- the specified ID the returned ID is.
--------------------------------------------------------
CREATE FUNCTION [dbo].[asi_GetCompanyRosterFromParentID] 
(	
     @p1 varchar(10)
)
RETURNS TABLE 
AS
RETURN 
(
    -- Get the parent and all children from any given ID
    WITH Roster ([ID], [Depth]) AS (
        SELECT [ID], 0 AS [Depth]
          FROM [dbo].[Name]
         WHERE [ID] = @p1
        UNION ALL
        SELECT n.[ID], [Depth] + 1
          FROM Roster r  
               INNER JOIN [dbo].[Name] n ON n.[CO_ID] = r.[ID] AND n.ID <> n.CO_ID
    )
    SELECT [ID], [Depth]
      FROM Roster  
)



