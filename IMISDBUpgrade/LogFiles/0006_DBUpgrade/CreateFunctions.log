    DROP FUNCTION [dbo].[asi_GetPercentileRank]

SET ANSI_DEFAULTS ON
SET IMPLICIT_TRANSACTIONS OFF
SET CONCAT_NULL_YIELDS_NULL ON
SET ARITHABORT ON
SET NUMERIC_ROUNDABORT OFF

CREATE FUNCTION [dbo].[asi_GetPercentileRank]
(@engagementScoreKey uniqueidentifier, @score decimal(18,4), @scoreYear int, @scoreMonth int, @scoreWeek int)
RETURNS int
AS
BEGIN
    DECLARE @numberOfRecords decimal(18,4)
    DECLARE @scoresLess decimal(18,4)
    DECLARE @scoresEqual decimal(18,4)
    DECLARE @result int

    SELECT @numberOfRecords = SUM ([Frequency]) FROM [dbo].[EngagementPercentile] 
     WHERE [EngagementScoreKey] = @engagementScoreKey    
       AND [ScoreYear] = @scoreYear   
       AND [ScoreMonth] = @scoreMonth   
       AND [ScoreWeek] = @scoreWeek

    SELECT @scoresLess = SUM ([Frequency]) FROM [dbo].[EngagementPercentile]
     WHERE [Score] < @score  
       AND [EngagementScoreKey] = @engagementScoreKey   
       AND [ScoreYear] = @scoreYear 
       AND [ScoreMonth] = @scoreMonth   
       AND [ScoreWeek] = @scoreWeek
       
    SELECT @scoresEqual = SUM ([Frequency]) FROM [dbo].[EngagementPercentile]  
     WHERE [Score] = @score  
       AND [EngagementScoreKey] = @engagementScoreKey  
       AND [ScoreYear] = @scoreYear 
       AND [ScoreMonth] = @scoreMonth   
       AND [ScoreWeek] = @scoreWeek
     
    IF @numberOfRecords = 0
        SET @result = 0
    ELSE
        SET @result = FLOOR(((ISNULL(@scoresLess,0) + (.5 * ISNULL(@scoresEqual,0))) / @numberOfRecords) * 100)
    
    RETURN @result
END



