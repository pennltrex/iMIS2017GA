--------------------------------------------------------
-- Get the roster of a child and its parents using
-- the Name.CO_ID relationship. Returns the ID, plus
-- the ID of the directly referenced parent (CO_ID),
-- and so on, recursively, until it finds the root.
-- Also returns a 'depth' to show how far removed from
-- the specified ID the returned ID is.
--------------------------------------------------------
CREATE FUNCTION [dbo].[asi_GetCompanyRosterFromChildID] 
(        
    @p1 varchar(10)
)
RETURNS TABLE 
AS
RETURN 
(
    -- Get the child and all parents from any given ID
    WITH Roster ([ID], [CO_ID], [Depth]) AS (
        SELECT [ID], [CO_ID], 0 AS [Depth]
          FROM [dbo].[Name]
         WHERE [ID] = @p1
        UNION ALL
        SELECT n.[ID], n.[CO_ID], [Depth] + 1
          FROM Roster r  
               INNER JOIN [dbo].[Name] n ON n.[ID] = r.[CO_ID] AND n.[ID] <> n.[CO_ID]
    )
    SELECT [ID], [Depth]
      FROM Roster
)



IF  EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'[dbo].[asi_GetCompanyRosterFromParentID]') AND type in (N'FN', N'IF', N'TF', N'FS', N'FT'))
