-- given a DocumentVersionKey and a RootHierarchyKey, gets the full path to it as a string
CREATE FUNCTION [dbo].[asi_DocumentPath](@documentVersionKey uniqueidentifier, @rootHierarchyKey uniqueidentifier)
RETURNS nvarchar(4000)
AS
BEGIN
    DECLARE @hierarchyKey uniqueidentifier;
    DECLARE @documentName nvarchar(100);
    DECLARE @path nvarchar(4000);

    SELECT TOP 1 
           @hierarchyKey = h.HierarchyKey,
           @documentName = DocumentName 
      FROM [dbo].[Hierarchy] h
           INNER JOIN [dbo].[HierarchyRoot] hr ON h.[RootHierarchyKey] = hr.[RootHierarchyKey]
           INNER JOIN [dbo].[DocumentMain] dm ON h.[UniformKey] = dm.[DocumentVersionKey]
     WHERE h.[UniformKey] = @documentVersionKey AND h.RootHierarchyKey = @rootHierarchyKey
     ORDER BY dm.[UpdatedOn] DESC, dm.[DocumentStatusCode] ASC

    IF @@ROWCOUNT > 0
    BEGIN
       SELECT @path = [dbo].[asi_DocumentPathFromHierarchyAndRootKey](@hierarchyKey, @rootHierarchyKey)
       IF @hierarchyKey <> @rootHierarchyKey
          SET @path = @path + N'/' + @documentName      
    END

    RETURN @path
END




