 create function dbo.fn_asi_convert_foreign_string(@instring varchar(2000))
 returns varchar(2000)
 as
 begin
 	declare @i int
 	declare @str_len int
 	declare @outstring varchar(2000)
 	set @str_len = len(@instring)
 	set @i = 1
