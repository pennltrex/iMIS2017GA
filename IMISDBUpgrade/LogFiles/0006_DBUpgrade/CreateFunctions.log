-- =============================================
-- This is a Levenshtein algorithm that will compare two phrases and return
-- the amount of steps (distance) it takes for phrase B to match phrase A.
-- This function is used to provide a numerical weight to a group of 
-- parties that might be duplicates of each other. Used by the
-- asi_DuplicateEngineWeighCandidates stored procedure.
-- =============================================
CREATE FUNCTION [dbo].[asi_Edit_Distance] (
    @s1 nvarchar(max), 
    @s2 nvarchar(max)
)
RETURNS int
AS
BEGIN
    DECLARE @s1_len int;
    DECLARE @s2_len int;
    DECLARE @i int;
    DECLARE @j int;
    DECLARE @s1_char nchar;
    DECLARE @c int;
    DECLARE @c_temp int;
    DECLARE @cv0 varbinary(max);
    DECLARE @cv1 varbinary(max);
    
    SELECT @s1_len = LEN(@s1),
           @s2_len = LEN(@s2),
           @cv1 = 0x0000,
           @j = 1,
           @i = 1,
           @c = 0;

    WHILE @j <= @s2_len
        SELECT @cv1 = @cv1 + CAST(@j AS binary(2)),
               @j = @j + 1;
               
    WHILE @i <= @s1_len
    BEGIN
        SELECT @s1_char = SUBSTRING(@s1, @i, 1),
               @c = @i,
               @cv0 = CAST(@i AS binary(2)),
               @j = 1;

        WHILE @j <= @s2_len
        BEGIN
            SET @c = @c + 1;
            SET @c_temp = CAST(SUBSTRING(@cv1, @j + @j - 1, 2) AS int) + CASE WHEN @s1_char = SUBSTRING(@s2, @j, 1)
                                                                              THEN 0
                                                                              ELSE 1
                                                                         END;
            IF @c > @c_temp
            BEGIN 
                SET @c = @c_temp
            END;
            
            SET @c_temp = CAST(SUBSTRING(@cv1, @j + @j + 1, 2) AS int) + 1;
            IF @c > @c_temp
            BEGIN 
                SET @c = @c_temp
            END;
            
            SELECT @cv0 = @cv0 + CAST(@c AS binary(2)),
                   @j = @j + 1;
        END;
        SELECT @cv1 = @cv0,
               @i = @i + 1;
    END;

    RETURN @c;

END;




